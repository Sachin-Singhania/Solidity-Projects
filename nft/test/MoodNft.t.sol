pragma solidity ^0.8.13;
import {Test,console} from "forge-std/Test.sol";
import {DeployMoodNft} from "script/DeployMoodNft.s.sol";
import {MoodNft} from "src/MoodNft.sol";
contract MoodNfttest is Test{
    MoodNft public moodNft;
    DeployMoodNft public deployMoodNft;
    address public USER= makeAddr("uesr");
     string public constant HAPPY_SVG_URI =
        "data:application/json;base64,";
    string public constant SAD_SVG_URI =
        "data:application/json;base64,";
    function setUp() public {
        deployMoodNft= new DeployMoodNft();
        moodNft= deployMoodNft.run();
    }
   
   function testViewTokenURIIntegration() public {
     vm.prank(USER);
    moodNft.mintNft();
    console.log(moodNft.tokenURI(0));
    }

    function testFilpTokentoSad() public {
        vm.prank(USER);
        moodNft.mintNft();
        vm.prank(USER);
        moodNft.flipmood(0);
        assertEq( keccak256(abi.encodePacked(moodNft.tokenURI(0))),keccak256(abi.encodePacked(SAD_SVG_URI)));
    }
 }